mf i = 0 fr
mf a = [] fr

yolo i < 4 {
    mf v = i fr

    a[thicc(a)] = lit () {
        io.println("Captured value is \(v)") fr
    } fr

    i = i + 1 fr
}

yall _, f amongus a {
    f() fr
}

mf a = [] fr

yall i amongus 4, 0 {
    a[thicc(a)] = lit () {
        io.println("Captured value is \(i)") fr
    } fr
}

yall _, f amongus a {
    f() fr
}
